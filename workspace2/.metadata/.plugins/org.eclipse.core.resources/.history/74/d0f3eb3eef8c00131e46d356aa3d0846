
public class foo extends Hobbit {
	
	 public static void main(String[] args) { 
		 Hobbit bc=new Hobbit();
	     Short myGold = 7; 
	     bc.countGold(myGold, 6);
	     int i=0;
	     for(int j=0;j<=100;++j){
	    	i++;
	    	 System.out.println("old:"j);
	     }
	     System.out.println(i); 
	     
	   } 
}



